<h1>SIGNUP</h1>
<%= form_with(model: @user, url: users_path) do |f|%>
    <%= f.label :name %></br>
    <%= f.text_field :name %></br>
    
    <%= f.label :height %></br>
    <%= f.number_field :height %></br>
    
    <%= f.label :happiness %></br>
    <%= f.number_field :happiness %></br>
    
    <%= f.label :nausea %></br>
    <%= f.number_field :nausea %></br>

    
    <%= f.label :tickets %></br>
    <%= f.number_field :tickets %></br>
    
    <%= f.label :password %></br>
    <%= f.password_field :password %></br>

    <%= f.label :admin, "Check box if you are an admin" %>
    <%= f.check_box :admin %>

    <%= f.submit %>
<% end %>