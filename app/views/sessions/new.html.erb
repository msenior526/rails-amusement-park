<h1>SIGNIN</h1>
<%= form_with(url: signin_path) do |f|%>
    <%= f.label :user_name %></br>
    <%= f.collection_select :user_name, User.all, :name, :name  %></br>
    
    <%= f.label :password %></br>
    <%= f.password_field :password %></br>

    <%= f.label :admin, "Check box if you are an admin" %>
    <%= f.check_box :admin %>

    <%= f.submit "Sign In"%>
<% end %>